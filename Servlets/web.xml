<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" id="WebApp_ID" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<display-name>Login</display-name>
<servlet>
<servlet-name>Validate</servlet-name>
<servlet-class>student.Validate</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Validate</servlet-name>
<url-pattern>/Validate</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Signup</servlet-name>
<servlet-class>student.Signup</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Signup</servlet-name>
<url-pattern>/Signup</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Logout</servlet-name>
<servlet-class>student.Logout</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Logout</servlet-name>
<url-pattern>/Logout</url-pattern>
</servlet-mapping>
<welcome-file-list>
<welcome-file>index.html</welcome-file>
<welcome-file>index1.html</welcome-file>
</welcome-file-list>
</web-app>